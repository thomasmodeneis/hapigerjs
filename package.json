  "dependencies": {
    "bluebird": "^3.0.6",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "lodash": "^3.0.1",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-mocha": "^2.2.0",
